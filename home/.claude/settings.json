{
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "1",
    "OTEL_METRICS_EXPORTER": "otlp"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Read(~/Developer/**)",
      "Bash(docker compose exec:*)",
      "Bash(docker run:*)",
      "Bash(find:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git switch:*)",
      "Bash(git fetch:*)",
      "Bash(git rebase:*)",
      "Bash(git stash:*)",
      "Bash(grep:*)",
      "Bash(rg:*)",
      "Fetch(https://docs.anthropic.com/*)",
      "Fetch(https://github.com/*)"
    ]
  },
  "theme": "sonnet",
  "model": "opus"
}

